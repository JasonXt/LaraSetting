{
    "name": "jason-xt/lara-setting",
    "type": "library",
    "description": "A persistent settings package for Laravel 5.5+",
    "keywords": ["laravel", "persistent", "setting"],
    "homepage": "https://github.com/JasonXt/LaraSetting/",
    "license": "MIT",
    "authors": [
        {
            "name": "xutao",
            "email": "649223152@qq.com",
            "homepage": "https://github.com/JasonXt/LaraSetting/",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "~5.5",
        "php" : "~7.0"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "JasonXt\\LaraSetting\\": "src"
        },
        "files":[
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JasonXt\\LaraSetting\\": "tests"
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "JasonXt\\LaraSetting\\LaraSettingServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
